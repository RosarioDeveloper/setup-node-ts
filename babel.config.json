{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@Models": "./src/models",
          "@Controllers": "./src/controllers"
        }
      }
    ]
  ],
  "ignore": ["**/*.spec.ts"]
}
